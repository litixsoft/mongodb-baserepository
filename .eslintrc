{
    "env": {
        "node": true
    },
    "rules": {
        "block-scoped-var": 2,
        "brace-style": [2, "1tbs"],
        "camelcase": 0,
        "consistent-this": 0,
        "default-case": 2,
        "eol-last": 0,
        "func-style": [2, "declaration"],
        "guard-for-in": 2,
        "handle-callback-err": 0,
        "no-catch-shadow": 2,
        "no-div-regex": 2,
        "no-floating-decimal": 2,
        "no-else-return": 2,
        "no-eq-null": 2,
        "no-extra-parens": 2,
        "no-lonely-if": 2,
        "no-self-compare": 2,
        "no-shadow": 1,
        "no-trailing-spaces": 2,
        "no-underscore-dangle": 0,
        "no-undefined": 0,
        "quotes": [2, "single"],
        "radix": 2,
        "space-after-keywords": 2,
        "space-unary-ops": [2, { "words": true, "nonwords": false }],
        "valid-jsdoc": [0, {
            "requireReturn": false
        }],
        "wrap-iife": [2, "any"]
    },
    "globals": {
        "afterEach": false,
        "beforeEach": false,
        "expect": false,
        "describe": false,
        "jasmine": false,
        "it": false,
        "spyOn": false,
        "window": false
    }
}
